#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/eslint@9.24.0/node_modules/eslint/bin/node_modules:/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/eslint@9.24.0/node_modules/eslint/node_modules:/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/eslint@9.24.0/node_modules:/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/eslint@9.24.0/node_modules/eslint/bin/node_modules:/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/eslint@9.24.0/node_modules/eslint/node_modules:/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/eslint@9.24.0/node_modules:/mnt/c/UMC_8th_Web/8th_Web_B/정-김세정/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@9.24.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@9.24.0/node_modules/eslint/bin/eslint.js" "$@"
fi
